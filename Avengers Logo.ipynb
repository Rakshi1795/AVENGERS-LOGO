    "import turtle as t\n",
    "\n",
    "t.bgcolor=('black')\n",
    "\n",
    "def circle(radius,color):\n",
    "    t.color(color)\n",
    "    t.begin_fill()\n",
    "    t.circle(radius)\n",
    "    t.end_fill()\n",
    "    \n",
    "t.goto(-30,-280)\n",
    "circle(300,'red')\n",
    "\n",
    "t.goto(-30,-240)\n",
    "circle(260,'white')"
   ]
